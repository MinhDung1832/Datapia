<body>
    <a id="button_top">
        <i class="uil-arrow-up" style="font-size: 50px; color: #fff;"></i>
    </a>
    <div class="wrapper">

        @*<div id="nav_top" style="display:none">
                @Html.Partial("~/Views/Shared/_NavTop.cshtml")
                @Html.Partial("~/Views/Shared/_MenuTop.cshtml")
            </div>*@
        <!-- ========== Topbar Start ========== -->
        @Html.Partial("~/Views/Shared/_Navbar.cshtml")
        @*@Html.Partial("~/Views/Shared/_NavTop.cshtml")*@
